  "bulkActionEndpoints": [
    {
      "endpoint": "POST /cases/bulkactions/attachfile",
      "description": "Attach file to multiple cases",
      "payload": {
        "caseSearch": "CaseSearchFilters",
        "fileContents": "string (base64)",
        "fileName": "string",
        "reference": "string",
        "timestamp": "string"
