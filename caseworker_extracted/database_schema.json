        "category": { "type": "belongsTo", "entity": "CategoryTypes", "foreignKey": "categoryTypeID" },
        "assignedTo": { "type": "belongsTo", "entity": "Caseworkers", "foreignKey": "assignedToID" },
        "casenotes": { "type": "hasMany", "entity": "Casenotes", "foreignKey": "caseID" },
        "tags": { "type": "belongsToMany", "entity": "Tags", "through": "CaseTags" },
        "reviewDates": { "type": "hasMany", "entity": "ReviewDates", "foreignKey": "caseID" },
        "files": { "type": "hasMany", "entity": "Files", "foreignKey": "caseID" },
        "letters": { "type": "hasMany", "entity": "Letters", "foreignKey": "caseID" }
      },
      "apiEndpoints": [
        "GET /cases/:id",
