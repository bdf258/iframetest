        "statusID": { "type": "integer", "nullable": true, "foreignKey": "StatusTypes.id" },
        "categoryTypeID": { "type": "integer", "nullable": true, "foreignKey": "CategoryTypes.id" },
        "assignedToID": { "type": "integer", "nullable": true, "foreignKey": "Caseworkers.id" },
